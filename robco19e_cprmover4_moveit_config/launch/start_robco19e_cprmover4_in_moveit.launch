<launch>

<!-- The robot should be set to the four axis mover -->
 <param name="robot_type" value="robco19e"/>
	<!--<param name="robot_description" textfile="$(find cpr_rviz_plugin)/CPRMover4.urdf.xacro" /> ???? -->
 <!--<node name="cpr_mover" pkg="$(find cpr_mover)" type="cpr_mover"/>-->

  <!-- By default, we do not start a database (it can be large) -->
  <arg name="db" default="false" />
  <!-- Allow user to specify database location -->
  <arg name="db_path" default="$(find robco19e_cprmover4_moveit_config)/default_warehouse_mongo_db" />

  <!-- By default, we are not in debug mode -->
  <arg name="debug" default="false" />

  <!-- Load the URDF, SRDF and other .yaml configuration files on the param server -->
  <include file="$(find robco19e_cprmover4_moveit_config)/launch/planning_context.launch">
    <arg name="load_robot_description" value="true"/>
  </include>




  <!-- Run the main MoveIt executable without trajectory execution (we do not have controllers configured by default) -->
  <include file="$(find robco19e_cprmover4_moveit_config)/launch/move_group.launch">
    <arg name="allow_trajectory_execution" value="true"/>
    <arg name="fake_execution" value="false"/>
    <arg name="info" value="true"/>
    <arg name="debug" value="$(arg debug)"/>
  </include>


      <!--ADD this to base link????-->
	<node name="robot_state_publisher" pkg="robot_state_publisher" type="state_publisher" />

<!--By Me ................................-->
<!--<include file="$(find robco19e_cprmover4_moveit_config)/launch/joystick_control.launch"/>-->
<!--...................-->

  <!-- Run Rviz and load the default config to see the state of the move_group node -->
  <include file="$(find robco19e_cprmover4_moveit_config)/launch/moveit_rviz.launch">
    <arg name="config" value="true"/>
    <arg name="debug" value="$(arg debug)"/>
  </include>

  <!-- If database loading was enabled, start mongodb as well -->
  <include file="$(find robco19e_cprmover4_moveit_config)/launch/default_warehouse_db.launch" if="$(arg db)">
    <arg name="moveit_warehouse_database_path" value="$(arg db_path)"/>
  </include>

</launch>
